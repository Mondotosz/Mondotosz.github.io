{
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19"
  },
  "scripts": {
    "dev" : "npx tailwindcss -i ./css/tailwind.css -o ./css/style.css",
